default_platform(:ios)

platform :ios do

    private_lane :api_key do
        api_key = app_store_connect_apikey(
        #configure through environment variables
            in_house: false
        )
    end

    lane :flight do
        pilot(
            skip_waiting_for_build_processing: true,
            api_key:api_key,
            ipa: "./output/newbuild.ipa",
        )
    end

end